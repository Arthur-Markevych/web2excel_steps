  for ( var i = 0; i < hard.length; i++){
  //ОТОБРАЗИТЬ СПИСОК СТОЛЯРНОЙ МЕБЕЛИ
    $("#testTable").append('<tr class="' + hard[i]['item'] + '"><td class="check"><input class="checkbox" type="checkbox"></td><td><img alt="" src="' + hard[i]['img'] + '" /></td><td class="name">' + hard[i]['name'] + '<br><span>height: ' + hard[i]['height'] + ' mm</span><br><span>width: ' + hard[i]['width'] + ' mm</span><br><span>depth: ' + hard[i]['depth'] + ' mm</span><br><span>volume: ' + hard[i].volume() + ' sq.m.</span></td><td style="opacity:0;" class="get_furnish"><div><select class="furnish"><option value="fabric">Fabric</option></select><p class="after"></p></div></td><td style="opacity:0;" class="category"><div><select class="get_category"><option value="1">1</option></select><p class="add_category"></p><p class="after"></p></div></td><td style="opacity:0;" class="get_material"><div><select class="material"><option value="name 1">name 1</option></select>'+ wood_names + '<td class="price"></td><td class="price_plus"><td class="discount"></td></td><td class="amount"><input class="get_amount" type="number" min="1" max="200" step="1" value ="1"/></td><td class="total"></td><td class="delivery"></td><td class="unvisible">' + hard[i]['price_fabric'] + '</td><td class="unvisible">' + hard[i]['price_leather'] + '</td><td class="unvisible">' + hard[i]['price_suede'] + '</td></tr>');
  }

  all_rows = $('table tr').length;
  for (var i = 1; i < all_rows; i++){
    $('table tr').eq(i).hide();
  }

  // console.log(all_rows);


  showcases = $("table tr.showcases").last().index();

  //СКРЫТЬ ПОВТОРЯЮЩИЕСЯ ЭЛЕМЕНТЫ ПОСЛЕ ТОГО, КАК СФОРМИРОВАНА ТАБЛИЦА
  for (var i = 0; i < hidden_items.length; i = i+2){
    for (var k = 0; k < hidden_items[i+1]; k++){
        $('table tr').eq(hidden_items[i]+k+2).css('display', 'none');
    }
  }

  for (key in fields){
    fields_length++;
  }

});


//ИЗМЕНЕНИЕ КОЭФФИЦИЕНТА!!!
$(document).on('change','input[class=index]',function(){
  index = +$('.index').val();
  var rows = $('table tr').length;
  var array = [];
  var j = 0;
  for (var i = 1; i < rows; i++){
    if ($('table tr').eq(i).find('td').eq(fields['check']).find('input').prop('checked')){
      array[j] = i;
      j++
    }
  }

  for (var i = 0; i < array.length; i++){
    var category = $('table tr').eq(array[i]).find('td').eq(fields['category']).find('.get_category').prop('value');
    var furnish = $('table tr').eq(array[i]).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
    var finish = $('table tr').eq(array[i]).find('td').eq(fields['finishes']).find('.wood').prop('value');
    var amount = $('table tr').eq(array[i]).find('td').eq(fields['amount']).find('.get_amount').val();

    sum(furnish, category, finish, amount, array[i]-1, index, discount);
  }

});


//ИЗМЕНЕНИЕ СКИДКИ!!!
$(document).on('change','input[class=discount]',function(){
  discount = 100 - +parseInt($('.discount').val());
  $('.discount').val(100 - discount + "%");

  var rows = $('table tr').length;
  var array = [];
  var j = 0;
  for (var i = 1; i < rows; i++){
    if ($('table tr').eq(i).find('td').eq(fields['check']).find('input').prop('checked')){
      array[j] = i;
      j++;
    }
  }

  for (var i = 0; i < array.length; i++){
    var category = $('table tr').eq(array[i]).find('td').eq(fields['category']).find('.get_category').prop('value');
    var furnish = $('table tr').eq(array[i]).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
    var finish = $('table tr').eq(array[i]).find('td').eq(fields['finishes']).find('.wood').prop('value');
    var amount = $('table tr').eq(array[i]).find('td').eq(fields['amount']).find('.get_amount').val();

    sum(furnish, category, finish, amount, array[i]-1, index, discount);
  }

});


//ИЗМЕНЕНИЕ СТОИМОСТИ ДОСТАВКИ!!!
$(document).on('change','input[class=delivery_index]',function(){
  delivery = +parseInt($('.delivery_index').val());
  $('.delivery_index').val(delivery + '€');

  var rows = $('table tr').length;
  var array = [];
  var j = 0;

  for (var i = 1; i < rows; i++){
    if ($('table tr').eq(i).find('td').eq(fields['check']).find('input').prop('checked')){
      array[j] = i;
      j++
    }
  }

  for (var i = 0; i < array.length; i++){
    change_delivery(delivery, array[i]);
  }

});


function change_delivery(a, b){
    var delivery = a;
    var row_index = b;

    if (row_index <= fluffy.length){
        var amount = $('table tr').eq(row_index).find('td').eq(fields['amount']).find('.get_amount').val();
        $('table tr').eq(row_index).find('td').eq(fields['delivery']).html((fluffy[row_index-1].volume()*delivery*amount).toFixed(0));
    }else{
        var amount = $('table tr').eq(row_index).find('td').eq(fields['amount']).find('.get_amount').val();
        $('table tr').eq(row_index).find('td').eq(fields['delivery']).html((hard[row_index-fluffy.length-1].volume()*delivery*amount).toFixed(0));
    }
}

//ИЗМЕНЕНИЕ НАЗВАНИЯ КОМНАТЫ!!!
$(document).on('change','div[id=room_name] input',function(){

  current_room_name = $('#room_name input').val();

});


//ИЗМЕНЕНИЕ ФЛАЖКА!!!
$(document).on('change','input[class=checkbox]',function(){
  var row_index = $(this).parent().parent().index();
  var check = $(this).prop('checked');
  var delivery = +parseInt($('.delivery_index').val());

  if (check == true){
    $('table tr').eq(row_index+1).css('background-color', '#ccffff');
    var furnish = $('table tr').eq(row_index+1).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
    var category = $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.get_category').prop('value');
    var finish = $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.wood').prop('value');
    var amount = $('table tr').eq(row_index+1).find('td').eq(fields['amount']).find('.get_amount').val();

    if (row_index < fluffy.length){
         var delivery_price = (fluffy[row_index].volume()*delivery).toFixed(0);
        $('table tr').eq(row_index+1).find('td').eq(fields['delivery']).html(delivery_price);

        // (fluffy[row_index-1].volume()*delivery*amount).toFixed(0)


        sum(furnish, category, finish, amount, row_index, index, discount);

    }else if (row_index >= fluffy.length){
        row_index = row_index - fluffy.length;
        var delivery_price = +(hard[row_index].volume()*delivery).toFixed();
        $('table tr').eq(row_index+fluffy.length+1).find('td').eq(fields['delivery']).html(delivery_price);
        sum(furnish, category, finish, amount, row_index+fluffy.length, index, discount);
    }

  } else {
    $('table tr').eq(row_index+1).css('background-color', '#fff');

    reset(row_index);
  }

});


//ПОКАЗАТЬ/СКРЫТЬ ДОПОЛНИТЕЛЬНЫЕ ЭЛЕМЕНТЫ КОЛЛЕКЦИИ нажатием на -/*!!!
$(document).on('click','div[class=plus]',function(){
  var row_index = $(this).parent().parent().index();
  var start = row_index+1;
  var end = fluffy[row_index]['hide'] + row_index +1;

  if ($('table tr').eq(row_index+1).find('td').eq(fields['check']).find('div').find('i').hasClass("fa-plus")){
    $('table tr').eq(row_index+1).find('td').eq(fields['check']).find('div').html('<i class="fa fa-minus" aria-hidden="true"></i>');
    for (var i = start; i <= end; i++){
        $('table tr').eq(i).show('slow');
    }
  }else {
    $('table tr').eq(row_index+1).find('td').eq(fields['check']).find('div').html('<i class="fa fa-plus" aria-hidden="true"></i>');
    for (var i = start+1; i <= end; i++){
        $('table tr').eq(i).hide('slow');
    }
  }
});


//ИЗМЕНЕНИЕ ТКАНЬ/КОЖА/ЗАМША!!!
$(document).on('change','select[class=furnish]',function(){

  var row_index = $(this).parent().parent().parent().index();
  var check = $('table tr').eq(row_index+1).find('td').eq(fields['check']).find('.checkbox').prop('checked');
  var furnish = $('table tr').eq(row_index+1).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
  var finish = $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.wood').prop('value');
  var amount = $('table tr').eq(row_index+1).find('td').eq(fields['amount']).find('.get_amount').val();

  change_category(row_index, furnish);
  var category = $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.get_category').prop('value');

  if (check == true){
    sum(furnish, category, finish, amount, row_index, index, discount);
  }
});

//ФОРМИРОВАНИЕ СПИСКА МАТЕРИАЛОВ ПРИ СМЕНЕ МАТЕРИАЛА И КАТЕГОРИИ
function change_category(a, b){
    var row_index = a;
    var furnish = b;

    var category_html = '<div><select class="get_category">';
    var material_html = '<div><select class="material">';

    for (var i = 1; i <= my_object[furnish]['category'].length; i++){
        category_html = category_html + '<option value="' + i + '">' + i + '</option>';
    }
    category_html = category_html + '</select><p class="add_category"></p><p class="after"></p></div>';
    $('table tr').eq(row_index+1).find('td').eq(fields['category']).html(category_html);

    for (var i = 0; i < my_object[furnish]['1'].length; i++){
        material_html = material_html + '<option value="name ' + i+1 + '">' + my_object[furnish]['1'][i] + '</option>';
    }
    material_html = material_html + '</select></div>';
    $('table tr').eq(row_index+1).find('td').eq(fields['name_of_material']).html(material_html);

}

function change_material(a, b, c){
    var row_index = a;
    var furnish = b;
    var category = c;
    var material_html = '<div><select class="material">';

    for (var i = 0; i < my_object[furnish][category].length; i++){
        material_html = material_html + '<option value="name ' + i+1 + '">' + my_object[furnish][category][i] + '</option>';
    }
    material_html = material_html + '</select></div>';
    $('table tr').eq(row_index+1).find('td').eq(fields['name_of_material']).html(material_html);
}


//ИЗМЕНЕНИЕ КАТЕГОРИИ ТКАНИ/КОЖИ/ЗАМШИ!!!
$(document).on('change','select[class=get_category]',function(){
  var row_index = $(this).parent().parent().parent().index();
  var col_index = $(this).parent().parent().index();
  var check = $('table tr').eq(row_index+1).find('td').eq(fields['check']).find('.checkbox').prop('checked');
  var category = $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.get_category').prop('value');
  var furnish = $('table tr').eq(row_index+1).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
  var finish = $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.wood').prop('value');
  var amount = $('table tr').eq(row_index+1).find('td').eq(fields['amount']).find('.get_amount').val();

  change_material(row_index, furnish, category);

  if (check == true){
    sum(furnish, category, finish, amount, row_index, index, discount);
  }
});



//ИЗМЕНЕНИЕ ДЕРЕВА!!!
$(document).on('change','select[class=wood]', function(){
  var row_index = $(this).parent().parent().parent().index();
  var col_index = $(this).parent().parent().index();
  var check = $('table tr').eq(row_index+1).find('td').eq(fields['check']).find('.checkbox').prop('checked');
  var category = $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.get_category').prop('value');
  var furnish = $('table tr').eq(row_index+1).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
  var finish = $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.wood').prop('value');
  var amount = $('table tr').eq(row_index+1).find('td').eq(fields['amount']).find('.get_amount').val();

  $('table tr').eq(row_index+1).find('.get_wood img').attr("src", my_object['Wood']['Wood_images'][finish-1]);


  if (check == true){
    sum(furnish, category, finish, amount, row_index, index, discount);
  }
});


//ИЗМЕНЕНИЕ КОЛИЧЕСТВА!!!
$(document).on('change','input[class=get_amount]',function(){
  var row_index = $(this).parent().parent().index();
  // var col_index = $(this).parent().index();
  var check = $('table tr').eq(row_index+1).find('td').eq(fields['check']).find('.checkbox').prop('checked');
  var category = $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.get_category').prop('value');
  var furnish = $('table tr').eq(row_index+1).find('td').eq(fields['upholstery']).find('.furnish option:selected').prop('value');
  var finish = $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.wood').prop('value');
  var amount = $('table tr').eq(row_index+1).find('td').eq(fields['amount']).find('.get_amount').val();

  if (check == true){

    sum(furnish, category, finish, amount, row_index, index, discount);
  }
});


//ПРОСЧЕТ СТРОКИ!!!
function sum(a, b, c, d, e, f, g){
  var furnish = a;
  var category = b;
  var finish = c;
  var amount = d;
  var row_index = e;
  var index = f;
  var discount = g;
  var delivery = +parseInt($('.delivery_index').val());

  if (row_index < fluffy.length){ //МЯГКАЯ МЕБЕЛЬ
    if (furnish == 'Fabric'){
        var price = +$('table tr').eq(row_index+1).find('td').eq(fields['fabric_price']).html();
        get_sum(price, index, discount);
      } else if (furnish == 'Leather'){
        var price = +$('table tr').eq(row_index+1).find('td').eq(fields['leather_price']).html();
        get_sum(price, index, discount);
      }else{
        var price = +$('table tr').eq(row_index+1).find('td').eq(fields['suede_price']).html();
        get_sum(price, index, discount);
      }
  }else{ //СТОЛЯРКА
    var price = +$('table tr').eq(row_index+1).find('td').eq(fields['fabric_price']).html();
    get_sum(price, index, discount);
  }


function get_sum(a, b, c){
    var cost = a;
    var index = b;
    var discount = c;

    if (row_index < fluffy.length){  //МЯГКАЯ МЕБЕЛЬ
        //считаем доставку
        $('table tr').eq(row_index+1).find('td').eq(fields['delivery']).html(+(fluffy[row_index].volume()*delivery*amount).toFixed(0));
        
        //копируем значение базовой цены в столбик price
        $('table tr').eq(row_index+1).find('td').eq(fields['price_start']).html(cost);

        $('table tr').eq(row_index+1).find('td').eq(fields['price_of_one_item']).html(Math.round(cost*index*my_object[furnish].category[category-1]*my_object.Wood[furnish][finish-1]));
        var price_of1 = Math.round((+$('table tr').eq(row_index+1).find('td').eq(fields['price_of_one_item']).html())*discount/100);
        $('table tr').eq(row_index+1).find('td').eq(fields['discount']).html(price_of1);
        $('table tr').eq(row_index+1).find('td').eq(fields['total_price']).html(price_of1*amount);

        if (my_object[furnish].category[category-1]*100-100 > 1){
          $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.add_category').html((my_object[furnish].category[category-1]*100-100).toFixed(0) + '%');
        }else{
          $('table tr').eq(row_index+1).find('td').eq(fields['category']).find('.add_category').html('');
        }

        if (my_object.Wood[furnish][finish-1]*100-100 > 1){
          $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.add_wood').html((my_object.Wood[furnish][finish-1]*100-100).toFixed(0) + '%');
        }else{
          $('table tr').eq(row_index+1).find('td').eq(fields['finishes']).find('.add_wood').html('');
        }
    }else{ //СТОЛЯРКА
        $('table tr').eq(row_index+1).find('td').eq(fields['price_start']).html(cost);
        $('table tr').eq(row_index+1).find('td').eq(fields['price_of_one_item']).html(Math.round(cost*index));
        $('table tr').eq(row_index+1).find('td').eq(fields['delivery']).html((hard[row_index-fluffy.length].volume()*delivery*amount).toFixed(0));
        var price_of1 = Math.round((+$('table tr').eq(row_index+1).find('td').eq(fields['price_of_one_item']).html())*discount/100);
        $('table tr').eq(row_index+1).find('td').eq(fields['discount']).html(price_of1);
        $('table tr').eq(row_index+1).find('td').eq(fields['total_price']).html(price_of1*amount);
    }

  }
}


//СКРЫТИЕ ПУНКТОВ МЕНЮ МЯГКОЙ МЕБЕЛИ при нажатии на иконку дивана!!!
$(document).on('click','span[id=sofa_icon]',function(){

  for ( var i = 0; i < buttons.length; i++){
    if (buttons[i]['item'] == 1){
      $('#'+buttons[i]['id']).toggle();
    }
  }

});

//СКРЫТИЕ ПУНКТОВ МЕНЮ СТОЛЯРКИ при нажатии на иконку тумба!!!
