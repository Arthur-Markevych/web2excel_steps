  for ( var i = 0; i < buttons.length; i++){
    if (buttons[i]['item'] == 2){
      $('#'+buttons[i]['id']).toggle();
    }
  }

});


//ВЫБОР ПУНКТА МЕНЮ с вызовом функции изменения цвета!!!
$(document).on('click','aside input[type=button]',function(){
  var my_id = $(this).prop('id');

  for (var i = 0; i < buttons.length; i++){
    if (buttons[i]['id'] == my_id){
        var start = buttons[i].start();
        var end = buttons[i].end();

        if ($('table tr').eq(start+1).css('display') == 'table-row'){
            for ( var j = start +1; j < end +2; j++){
                $('table tr').eq(j).hide();
            }
        }else if ($('table tr').eq(start+1).css('display') == 'none'){
            for ( var j = start +1; j < end +2; j++){
                if (j < fluffy.length){
                    if (fluffy[j-1]['hide']){
                        $('table tr').eq(j).show();
                        j = j + fluffy[j-1]['hide'];
                    }else{
                        $('table tr').eq(j).show();
                    }
                }else {
                    $('table tr').eq(j).show();
                }


            }
        }


    }
}

  change_color(my_id, start);

});



//ИЗМЕНЕНИЕ ЦВЕТА МЕНЮ!!!
//Функция, меняющая цвет кнопки при скрытии/появлении элементов списка
function change_color(a,b){
  var my_id = a;
  var start = b;
  if ($('table tr').eq(start +1).css('display') == 'none'){
      $('#'+my_id).css('background-color', '#eee');
  }else{
      $('#'+my_id).css('background-color', '#ccffff');
    }
}


//ФИЛЬТР!!!
//Фильтрация всех выбранных элементов +подсчет общей стоимости
$(document).on('click','#filter',function(){
    items_price = 0;
    delivery_price = 0;

  var array=[];
  var j = 0;

  for (var i = 1; i<= all_rows; i++){
    var check = $('table tr').eq(i).find('td').eq(fields['check']).find('.checkbox').prop('checked');

    if (check == true){
      array[j] = i;
      j++;
      $('table tr').eq(i).css('display', 'table-row');
    }else{
      $('table tr').eq(i).css('display', 'none');
    }
  }

  for (var i = 0; i < array.length; i++){
    var price = +$('table tr').eq(array[i]).find('td').eq(fields['total_price']).html();
    items_price = items_price + price;
    var delivery = +$('table tr').eq(array[i]).find('td').eq(fields['delivery']).html();
    delivery_price = delivery_price + delivery;
  }
  // total_total = total_total + delivery_price + items_price;

  $('#items_price').find('span').html(items_price + ' &#8364;');
  $('#delivery_price').find('span').html(delivery_price + ' &#8364;');
  $('#total_price').find('span').html(delivery_price+items_price + ' &#8364;');
});


//ПОКАЗАТЬ ВСЕ!!!
$(document).on('click','div[id=show_all] input',function(){
  for (var i = 0; i < all_rows; i++){
    $('table tr').eq(i).css('display', 'table-row');
  }

  $( "aside" ).find( "input.menu" ).css( "background-color", "#ccffff" );
});


//СБРОСИТЬ ФЛАЖКИ!!!
$(document).on('click','div[id=reset] input',function(){
    for (var i = 0; i < all_rows; i++){
        $('table tr').eq(i).find('td').eq(fields['check']).find('.checkbox').prop( "checked", false );
        $('table tr').eq(i).css('background-color', '#fff');
        if (i > 0){
            $('table tr').eq(i).css('display','none');
            reset(i-1);
        }
    }
    $('#items_price').find('span').html('');
    $('#delivery_price').find('span').html('');
    $('#total_price').find('span').html('');
    $( "aside" ).find( "input.menu" ).css( "background-color", "#eee" );
});

function reset(a){
    var i = a;

    $('table tr').eq(i+1).find('td').eq(fields['upholstery']).find('div select').val('Fabric');
    $('table tr').eq(i+1).find('td').eq(fields['category']).find('div select').val('1');
    $('table tr').eq(i+1).find('td').eq(fields['category']).find('div .add_category').html('');
    //При отсутствии флажка сформировать список материалов для ткани первой категории
    var material_html = '<div><select class="material">';
    for (var j = 0; j < my_object['Fabric']['1'].length; j++){
        material_html = material_html + '<option value="name ' + j + 1 + '">' + my_object['Fabric']['1'][j] + '</option>';
    }
    material_html = material_html + '</select></div>';
    $('table tr').eq(i+1).find('td').eq(fields['name_of_material']).html(material_html);

    $('table tr').eq(i+1).find('td').eq(fields['finishes']).find('div select').val('1');
    $('table tr').eq(i+1).find('.get_wood img').attr("src", my_object['Wood']['Wood_images'][0]);
    $('table tr').eq(i+1).find('td').eq(fields['finishes']).find('div .add_wood').html('');
    $('table tr').eq(i+1).find('td').eq(fields['price_start']).html(''); //цена за единицу без наценки
    $('table tr').eq(i+1).find('td').eq(fields['price_of_one_item']).html(''); //цена за единицу с наценкой
    $('table tr').eq(i+1).find('td').eq(fields['discount']).html('');
    $('table tr').eq(i+1).find('td').eq(fields['amount']).find('input').val('1');
    $('table tr').eq(i+1).find('td').eq(fields['total_price']).html(''); //общая цена
    $('table tr').eq(i+1).find('td').eq(fields['delivery']).html('');
}

//СОХРАНИТЬ КОМНАТУ!!!
$(document).on('click','div[id=save] input',function(){

    result = result + '<tr>' + room_header_name + $('#room_name input').val().toUpperCase() + '</th></tr>' + room_header_before_discount + '-' + (100 - discount + '%') + room_header_after_discount;

    var row_high_style = '<tr style="height: 300px; text-align: center; vertical-align: middle;">';
    var row_normal_hight = '<tr style="height: 160px; text-align: center; vertical-align: middle;">';
    var row_fluffy12_style = '<td style="border: 1px solid black; width: 280px; font-size: 16px;">';
    var row_fluffy_else_style = '<td style="border: 1px solid black; width: 100px; font-size: 16px;">';

for (var j = 1 ; j < all_rows; j++){
    if ($('table tr').eq(j).css('display') == 'none'){
      continue;
    }else if(j > fluffy.length){ //ЕСЛИ СТОЛЯРКА
        if (hard[j - fluffy.length - 1]['item'] == 'showcases'){ //ЕСЛИ ВИТРИНЫ, то высота строки 300px
            var row = row_high_style;
            for (i = 0; i < fields_length; i++){
                if (i == fields['photo'] || i == fields['name']){
                  row = row + row_fluffy12_style + $('table tr').eq(j).find('td').eq(i).html() + '</td>';
                }else if (i == fields['check'] || i == fields['price_start'] || i > fields['delivery']){
                  continue;
                }else if (i == fields['amount']){
                  row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(fields['amount']).find('.get_amount').val() + '</td>';
                }else if(i == fields['upholstery'] || i == fields['category'] || i == fields['name_of_material']){
                    row = row + row_fluffy_else_style + ' ' + '</td>';
                }else if(i == fields['finishes']){
                    row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(fields['finishes']).find('div select option:selected').text() + '</td>';
                }else {
                  row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(i).html() + '</td>';
                }
            }
        }else{ //ВСЕ, КРОМЕ ВИТРИН, высота строки 160px
            var row = row_normal_hight;
            for (i = 0; i < fields_length; i++){
                if (i == fields['photo'] || i == fields['name']){
                  row = row + row_fluffy12_style + $('table tr').eq(j).find('td').eq(i).html() + '</td>';
                }else if (i == fields['check'] || i == fields['price_start'] || i > fields['delivery']){
                  continue;
                }else if (i == fields['amount']){
                  row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(fields['amount']).find('.get_amount').val() + '</td>';
                }else if(i == fields['upholstery'] || i == fields['category'] || i == fields['name_of_material']){
                    row = row + row_fluffy_else_style + ' ' + '</td>';
                }else if(i == fields['finishes']){
                    row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(fields['finishes']).find('div select option:selected').text() + '</td>';
                }else {
                  row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(i).html() + '</td>';
                }
            }
        }
    }else{ //МЯГКАЯ МЕБЕЛЬ
      var row = row_normal_hight;
      for (i = 0; i < fields_length; i++){
        if (i == fields['photo'] || i == fields['name']){
          row = row + row_fluffy12_style + $('table tr').eq(j).find('td').eq(i).html() + '</td>';
        }else if (i == fields['check'] || i == fields['price_start'] || i > fields['delivery']){
          continue;
        }else if (i >= fields['upholstery'] && i < fields['finishes']){
          row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(i).find('div option:selected').prop('value') + '</td>';
        } else if (i == fields['amount']){
          row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(fields['amount']).find('.get_amount').val() + '</td>';
        }else if(i == fields['finishes']){
          row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(fields['finishes']).find('div select option:selected').text() + '</td>';
        }else {
          row = row + row_fluffy_else_style + $('table tr').eq(j).find('td').eq(i).html() + '</td>';
        }
      }
    }
    row = row + '</tr>';
    result = result + row;
  }

  result = result + room_footer_style1 + items_price + '</th></tr>';
  result = result + room_footer_style2 + delivery_price + '</th></tr>';
  var total = items_price + delivery_price;
  result = result + room_footer_style3 + total + '</th></tr><tr><td></td></tr><tr><td></td></tr>';

  total_total = total_total + total;

  $('#rooms_count').css('display', 'block');
  room_counter++;
  $('#rooms_count').html(room_counter);

});



function fnExcelReport(){
  var tab_text="<table>";
  var textRange; var j, i;
  var cells = document.getElementById('testTable').rows[0].cells.length;

  tab = document.getElementById('testTable'); // id of table

  result = result + room_footer_style4 + total_total + '</th></tr>';
  result = result + '</table>';

  var ua = window.navigator.userAgent;
  var msie = ua.indexOf("MSIE ");

  if (msie > 0 || !!navigator.userAgent.match(/Trident.*rv\:11\./)){
    txtArea1.document.open("txt/html","replace");
    txtArea1.document.write(result);
    txtArea1.document.close();
    txtArea1.focus();
    sa=txtArea1.document.execCommand("SaveAs",true,"Say Thanks to Sumit.xls");
  }else{
    sa = window.open('data:application/vnd.ms-excel,' + encodeURIComponent(result));
    return (sa);
  }
}




